{
  "name": "tree-view",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "better-npm-run watch:dev",
    "prod": "better-npm-run watch:prod",
    "analyze:prod": "better-npm-run analyze:prod",
    "build:prod": "better-npm-run build:prod",
    "build:ghPages": "better-npm-run build:ghPages"
  },
  "betterScripts": {
    "watch:dev": {
      "command": "npx webpack-dev-server --config webpack.config.ts",
      "env": {
        "NODE_ENV": "development",
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}"
      }
    },
    "watch:prod": {
      "command": "npx webpack-dev-server --config webpack.config.ts",
      "env": {
        "NODE_ENV": "production",
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}"
      }
    },
    "analyze:prod": {
      "command": "npx webpack-dev-server --config webpack.config.ts",
      "env": {
        "WITH_ANALYZE_MODE": true,
        "NODE_ENV": "production",
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}"
      }
    },
    "build:prod": {
      "command": "npx webpack --config webpack.config.ts",
      "env": {
        "NODE_ENV": "production",
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}"
      }
    },
    "build:ghPages": {
      "command": "npx webpack --config webpack.config.ts",
      "env": {
        "NODE_ENV": "production",
        "FOR_GH_PAGES": true,
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}"
      }
    }
  },
  "dependencies": {
    "@bem-react/classname": "^1.5.2",
    "@types/antd": "^1.0.0",
    "@types/autoprefixer": "^9.5.0",
    "@types/classnames": "^2.2.7",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^11.13.8",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.2",
    "@types/uuid": "^3.4.5",
    "@types/webpack": "^4.4.31",
    "@types/webpack-bundle-analyzer": "^2.13.1",
    "@types/webpack-merge": "^4.1.5",
    "antd": "^3.16.3",
    "autoprefixer": "^9.5.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lodash-decorators": "^6.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.1.0",
    "ts-loader": "^5.4.4",
    "ts-node": "^8.1.0",
    "typescript": "^3.5.1",
    "url-loader": "^2.0.1",
    "uuid": "^3.3.3",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-merge": "^4.2.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "tslint": "^5.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.0.0",
    "babel-plugin-import": "^1.11.0",
    "better-npm-run": "^0.1.1",
    "style-loader": "^0.23.1",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  }
}
